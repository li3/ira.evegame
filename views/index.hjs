    <div class="panel panel-primary">
      <div class="panel-heading"><i class="fa fa-space-shuttle"></i> Shipname Tag Generator</div>
      <div class="panel-body">
        <div ira-shipname-generator></div>
      </div>
    </div>

    <div class="panel panel-primary">
      <div class="panel-heading"><i class="fa fa-tasks"></i> Directional Scanner Shipname Validator</div>
      <div class="panel-body">
        <div ira-shipname-validator></div>
      </div>
    </div>

