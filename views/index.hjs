<!DOCTYPE html>
<html ng-app="app" ng-strict-di>
  <head>
    <title>Ira - Eve Tools</title>
    <link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" rel="stylesheet">
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="/stylesheets/style.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-1.11.2.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.0/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.2/rollups/md5.js"></script>
    <script src="/javascripts/application/angular.extensions.js"></script>
    <script src="/javascripts/application/ira.naming.module.js"></script>
    <script src="/javascripts/application/app.module.js"></script>
    <script src="/javascripts/home/home.controller.js"></script>
